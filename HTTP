host: hexlet.local

# Урок 4
telnet localhost 8080
POST /upload HTTP/1.1
Host: hexlet.local
Content-Type: text/plain
Content-Length: 15

my request body

# урок 5
telnet localhost 8080
POST /session/new HTTP/1.1
Host: hexlet.local
Content-Type: application/x-www-form-urlencoded
Content-Length: 30

username=admin&password=secret

# урок 6
telnet localhost 8080
